{
  "cli": {
    "version": ">= 16.7.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "NODE_ENV": "production",
        "NEWS_API_KEY": "$NEWS_API_KEY",
        "TRANSLATE_API_KEY": "$TRANSLATE_API_KEY",
        "TRANSLATE_API_KEY2": "$TRANSLATE_API_KEY2"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "NODE_ENV": "production",
        "NEWS_API_KEY": "$NEWS_API_KEY",
        "TRANSLATE_API_KEY": "$TRANSLATE_API_KEY",
        "TRANSLATE_API_KEY2": "$TRANSLATE_API_KEY2"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
